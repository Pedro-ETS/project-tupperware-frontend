{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "functions": {
            "api/**/*.{js,ts}": {
              "runtime": "nodejs16.x"
            }
          }
        }
      },
      {
        "src": "build",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/index.html" }
    ]
  }